<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Fortifications - Industrial</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
		        <li Class="PatchOperationReplace">
			        <xpath>Defs/ThingDef[defName="FT_Trench"]/researchPrerequisites</xpath>
			        <value>
				        <researchPrerequisites>
					        <li>FT_Concrete</li>
				        </researchPrerequisites>
			        </value>
                </li>
		        <li Class="PatchOperationReplace">
			        <xpath>Defs/ThingDef[defName="FT_Platform"]/researchPrerequisites</xpath>
			        <value>
				        <researchPrerequisites>
					        <li>FT_Concrete</li>
				        </researchPrerequisites>
			        </value>
                </li>
			</operations>
		</match>
	</Operation>
</Patch>
